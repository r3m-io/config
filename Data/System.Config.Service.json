{
    "System": {
        "Config": {
            "Service": [
                {
                    "#class": "System.Config.Service",
                    "git": {
                        "command": [
                            "git config --global core.autocrlf input",
                            "git config pull.rebase false",
                            "git config --global user.name \"Development team\"",
                            "git config --global user.email \"development@universeorange.com\""
                        ]
                    },
                    "cron": {
                        "start": "service cron start -f",
                        "restart": "service cron restart -f"
                    }
                }
            ]
        }
    }
}